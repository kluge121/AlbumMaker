<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/template1Guide1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.4458" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/template1Guide2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.333" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/template1Guide3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.666" />


    <ImageView
        android:id="@+id/template1Iv1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="2dp"
        android:scaleType="fitXY"
        android:src="@drawable/plus_photo"
        app:layout_constraintBottom_toBottomOf="@+id/template1Guide2"
        app:layout_constraintEnd_toEndOf="@id/template1Guide1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/template1Iv3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="2dp"
        android:layout_weight="0.33"
        android:scaleType="fitXY"
        android:src="@drawable/plus_photo"
        app:layout_constraintBottom_toBottomOf="@+id/template1Guide3"
        app:layout_constraintEnd_toEndOf="@id/template1Guide1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/template1Guide2" />


    <ImageView
        android:id="@+id/template1Iv6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="2dp"
        android:layout_weight="0.33"
        android:scaleType="fitXY"
        android:src="@drawable/plus_photo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@id/template1Guide1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/template1Guide3" />


    <ImageView
        android:id="@+id/template1Iv2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="2dp"
        android:scaleType="fitXY"
        android:src="@drawable/plus_photo"
        app:layout_constraintBottom_toBottomOf="@+id/template1Guide2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/template1Guide1"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/template1Iv4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="2dp"
        android:scaleType="fitXY"
        android:src="@drawable/plus_photo"
        app:layout_constraintBottom_toBottomOf="@+id/template1Guide3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/template1Guide1"
        app:layout_constraintTop_toTopOf="@id/template1Guide2" />

    <ImageView
        android:id="@+id/template1Iv5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="2dp"
        android:scaleType="fitXY"
        android:src="@drawable/plus_photo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/template1Guide1"
        app:layout_constraintTop_toTopOf="@id/template1Guide3" />


</androidx.constraintlayout.widget.ConstraintLayout>